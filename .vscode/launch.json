{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug MCP Weather Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/index.js",
      "env": {
        "OPENWEATHER_API_KEY": "${input:openweatherApiKey}"
      },
      "console": "integratedTerminal",
      "preLaunchTask": "Build MCP Server",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    }
  ]
}
